{
  "name": "task-manager",
  "version": "1.0.0",
  "description": "Complete MERN Task Manager Application",
  "scripts": {
    "install-all": "npm install && npm install --prefix backend && npm install --prefix frontend",
    "start": "concurrently \"npm run prod --prefix backend\" \"npm start --prefix frontend\"",
    "dev": "concurrently \"npm run dev --prefix backend\" \"npm start --prefix frontend\""
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jspdf": "^1.3.3",
    "chai": "^5.2.1",
    "concurrently": "^9.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "mocha": "^11.7.1",
    "sinon": "^21.0.0"
  },
  "proxy": "http://localhost:5001",
  "author": "Task Manager Team",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "dotenv": "^17.2.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "react-datepicker": "^8.5.0",
    "react-icons": "^5.5.0",
    "socket.io-client": "^4.8.1"
  }
