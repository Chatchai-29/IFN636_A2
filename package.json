{
  "name": "task-manager",
  "version": "1.0.0",
  "description": "Complete MERN Task Manager Application",
  "scripts": {
    "install-all": "npm install && npm install --prefix backend && npm install --prefix frontend",
    "start": "concurrently \"npm run prod --prefix backend\" \"npm start --prefix frontend\"",
    "dev": "concurrently \"npm run dev --prefix backend\" \"npm start --prefix frontend\""
  },
  "devDependencies": {
    "@types/jspdf": "^1.3.3",
    "chai": "^5.3.3",
    "chai-http": "^5.1.2",
    "concurrently": "^9.2.0",
    "cross-env": "^10.1.0",
    "mocha": "^11.7.4",
    "mochawesome": "^7.1.4",
    "mongodb-memory-server": "^10.2.1",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4"
  },
  "proxy": "http://localhost:5001",
  "author": "Task Manager Team",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "react-datepicker": "^8.5.0",
    "react-icons": "^5.5.0",
    "socket.io-client": "^4.8.1"
  }
}
