    "morgan": "^1.10.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "chai": "^5.2.0",
    "chai-http": "^5.1.2",
    "cross-env": "^7.0.3",
    "mocha": "^10.7.3",
    "mochawesome": "^7.1.3",
    "mongodb-memory-server": "^10.1.4",
